	public MainController(StudentOpService service) {
		this.service = service;
	}

	public String handle(StudentVO vo) throws Exception{
		
		StudentDTO dto = null;
		
		// Conver vo class object into DTO class object.
		
		// Use Service
		
		String result = service.register(dto);
		return null;
		
	}
}
